{
  "name": "rollup-plugin-closure-compiler-new",
  "version": "0.0.0",
  "description": "Rollup plugin that uses the up-to-date JS version of Closure Compiler to minify your code. Works with sourcemaps",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/standard 'src/*.js' && ./node_modules/.bin/mocha",
    "build": "./node_modules/.bin/rollup -c",
    "testbuild": "./node_modules/.bin/rollup --config rollup.config.test.js",
    "format": "./node_modules/.bin/standard --fix 'src/*.js'"
  },
  "keywords": [
    "closure-compiler",
    "closure-compiler-js"
  ],
  "author": "Ben Gubler <nebrelbug@gmail.com> (https://bengubler.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nebrelbug/rollup-plugin-closure-compiler-new.git"
  },
  "bugs": {
    "url": "https://github.com/nebrelbug/rollup-plugin-closure-compiler-new/issues"
  },
  "devDependencies": {
    "mocha": "^6.1.4",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "google-closure-compiler": "^20190415.0.0",
    "rollup": "^1.11.3"
  }
}
